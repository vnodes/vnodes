        }
    ],
    "version": "0.0.1",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./auth": {
            "types": "./dist/lib/auth/index.d.ts",
            "import": "./dist/lib/auth/index.js",
            "default": "./dist/lib/auth/index.js"
        },
        "./boot": {
            "types": "./dist/lib/boot/index.d.ts",
            "import": "./dist/lib/boot/index.js",
            "default": "./dist/lib/boot/index.js"
        },
        "./const": {
            "types": "./dist/lib/const/index.d.ts",
            "import": "./dist/lib/const/index.js",
            "default": "./dist/lib/const/index.js"
        },
        "./crypto": {
            "types": "./dist/lib/crypto/index.d.ts",
            "import": "./dist/lib/crypto/index.js",
            "default": "./dist/lib/crypto/index.js"
        },
        "./decorators": {
            "types": "./dist/lib/decorators/index.d.ts",
            "import": "./dist/lib/decorators/index.js",
            "default": "./dist/lib/decorators/index.js"
        },
        "./helpers": {
            "types": "./dist/lib/helpers/index.d.ts",
            "import": "./dist/lib/helpers/index.js",
            "default": "./dist/lib/helpers/index.js"
        },
        "./metadata": {
            "types": "./dist/lib/metadata/index.d.ts",
