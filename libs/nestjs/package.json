        "access": "public",
        "tag": "latest"
    },
    "author": {
        "email": "robert.brightline+vnodes-nestjs@gmail.com",
        "name": "Robert Brightline",
        "url": "https://vnodes.github.io/vnodes/libs/nestjs"
    },
    "keywords": [],
