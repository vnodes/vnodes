{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "Model Names",
    "$id": "modelName",
    "description": "A list of model names",
    "anyOf": [
        {
            "description": "Abstract models",
            "enum": [
                "WithId",
                "WithTimestamps",
                "WithNotes",
                "WithUuid",
                "WithGeneratedUuid"
            ]
        },
        {
            "description": "Auth models",
            "enum": [
                "User",
                "Role",
                "Permission",
                "AccessToken",
                "AccessTokenPermission",
                "Session",
                "Otp",
                "RolePermission",
                "UserRole"
            ]
        },
        {
            "description": "Inventory models",
            "enum": [
                "Product",
                "Price",
                "Quantity",
                "PriceLevel",
                "Store",
                "Discount"
            ]
        },
        {
            "description": "Contact models",
            "enum": [
                "Contact",
                "Address",
                "Email",
                "Phone"
            ]
        }
    ]
}