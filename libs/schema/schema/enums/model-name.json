{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "model name",
    "description": "A list of model names",
    "anyOf": [
        {
            "description": "Abstract models",
            "enum": [
                "WithId",
                "WithTimestamps",
                "WithNotes",
                "WithUuid",
                "WithGeneratedUuid"
            ]
        },
        {
            "description": "Auth models",
            "enum": [
                "Person",
                "Profile",
                "User",
                "Employee",
                "Customer",
                "Role",
                "RolePermission",
                "UserRole",
                "Permission",
                "AccessToken",
                "AccessTokenPermission",
                "Session",
                "Otp"
            ]
        },
        {
            "description": "Inventory models",
            "enum": [
                "Product",
                "Price",
                "Quantity",
                "PriceLevel",
                "Store",
                "Discount",
                "Category",
                "Department"
            ]
        },
        {
            "description": "Contact models",
            "enum": [
                "Contact",
                "Address",
                "Email",
                "Phone",
                "Website",
                "SocialMedia"
            ]
        },
        {
            "description": "Media",
            "enum": [
                "Media",
                "File",
                "Image",
                "Avatar",
                "Document"
            ]
        }
    ],
    "additionalProperties": false
}