{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "Input Component",
    "$id": "inputComponent",
    "description": "A list of input components",
    "enum": [
        "/// String inputs",
        "text",
        "textarea",
        "texteditor",
        "/// Number inputs",
        "number",
        "rate",
        "number-range",
        "/// Date inputs ",
        "date",
        "datetime",
        "time",
        "/// Boolean inputs",
        "switch",
        "switch-group",
        "checkbox",
        "checkbox-group",
        "/// Enum inputs",
        "select",
        "select-list",
        "autocomplete",
        "radio"
    ]
}