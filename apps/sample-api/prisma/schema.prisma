generator client {
  provider = "prisma-client"
  output   = "../src/app/prisma"
}

generator nestjs {
  provider = "nestjs"
}

datasource db {
  provider = "postgresql"
}


model Sample { 
  id        Int       @id @default(autoincrement())
  uuid      String    @unique @default(uuid(7))
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

}